    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^3.2.0",
    "style-loader": "^1.0.0",
